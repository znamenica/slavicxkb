xkb_symbols "as_cyr" {
    name[Group1]= "AllSlavic Cyr";

    // +percent
    // TODO replace ligatures with with MULTIKEY SEQ if any
    // TODO add "<>" for slipping notation
    // TODO add "*" for markdown/ or may be develop own fotmat for cyrillic
    key <LWIN> { [       Cyrillic_io,       Cyrillic_IO, Ukrainian_yi,        Ukrainian_YI         ] };
    key <AE01> { [            exclam,                 1, UA67D,               UA67E                ] }; // pajerok, kavyka
    key <AE02> { [          quotedbl,                 2, U0484,               braceleft            ] }; // kamora
    key <AE03> { [             U0301,                 3, U0300,               braceright           ] }; // oxia (ostreco), varia (čažiś)
    key <AE04> { [         semicolon,                 4, U04A9,               U04AA                ] }; // ҩ Ҩ
    key <AE05> { [             U0487,                 5, U0486,               U0483                ] }; // pokryťe, psili, titlo
    key <AE06> { [          question,                 6, U046D,               U046C                ] }; // ѭ Ѭ
    key <AE07> { [         parenleft,                 7, U0477,               U0476                ] }; // ѷ Ѷ
    key <AE08> { [        parenright,                 8, U0469,               U0468                ] }; // ѩ Ѩ
    key <AE09> { [             colon,                 9, bracketleft,         guillemotleft        ] };
    key <AE10> { [            period,                 0, bracketright,        guillemotright       ] };
    key <AE11> { [             minus,        underscore, U047D,               U047C                ] }; // - _ ѽ Ѽ
    key <AE12> { [             equal,              plus, U047F,               U047E                ] }; // = + ѿ Ѿ

    key <AD01> { [             UA647,             UA646, Cyrillic_je,         Cyrillic_JE          ] }; // ꙇꙆјЈ
