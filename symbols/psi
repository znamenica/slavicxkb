partial alphanumeric_keys
xkb_symbols "as_lat" {
    include "latin"

    name[Group1]= "AllSlavic Lat";

    // TODO replace ligatures with with MULTIKEY SEQ if any
    //    endash,       emdash ]	}; -- ---
    //        rightsinglequotemark, U2022 ] };
    //        rightarrow,        U2194 ]	};
    //        copyright,   registered ]	};
    //        doublelowquotemark, leftsinglequotemark ] };
    //        brightdoublequotemark, leftdoublequotemark ] };
    //questiondown
    // uring
    // notsign
    // ’
    // --
    // U2030
    //          U2248 ]	};
    //         ssharp
    //        onehalf,   logicaland ]	};
    //         threequarters ] };
    //         plusminus ]	};
    //      ellipsis,            U0335 ] }; // … (ellipsis), isčerk(stroke)
    key <LWIN>	{ [  asciitilde,      grave,        acute,       apostrophe ] };
    //key <TLDE>	{ [  asciitilde,      grave,        acute,       apostrophe ] };
    key <AE01>  { [      exclam,          1,        U0328,           degree ] }; // ! 1  ̨(ogonek) °(degree)
    key <AE02>  { [          at,          2,        U030C,         quotedbl ] }; // @ 2  ̌(haček/caron) "
    key <AE03>  { [       U0301,          3,   numbersign,  dead_circumflex ] }; // ́ (ostreco/acute) 3 #   ̂(circumflex)
    key <AE04>  { [       U0300,          4,       dollar,        semicolon ] }; //   ̀(grave/čašiś) 4 $ ;
    key <AE05>  { [     percent,          5,        U0307,            U0303 ] }; // % 5  ̇(abovedot/krapka)  ̃(tilde)
    key <AE06>  { [    question,          6,  asciicircum,            U030A ] }; // ? 6 ^  ̊(abovering/nadkolco)
    key <AE07>  { [   parenleft,          7,    ampersand,            U0306 ] }; // ( 7 &  ̆(breve/kratka)
    key <AE08>  { [  parenright,          8,     asterisk,            U0308 ] }; // ) 8 *  ̈(diaeresis/dvokrapka)
    key <AE09>  { [       colon,          9,       zacute,           Zacute ] }; // : 9 ź Ź
    key <AE10>  { [      period,          0,        U0131,          section ] }; // . 0 ı §
    key <AE11>	{ [       minus, underscore,        U030B,            U0304 ] }; // - _  ̋(dvostreco)  ̄(macron/nadčerk)
    key <AE12>	{ [       equal,       plus,        U030F,            U0327 ] }; // = +  ̏(dvočašiś)  ̧(cedilla)

    key <AD01>  { [           q,          Q,      eogonek,          Eogonek ] }; // q Q ę Ę
    key <AD02>  { [           w,          W,       sacute,           Sacute ] }; // w W ś Ś
    key <AD03>  { [           e,          E,       ecaron,           Ecaron ] }; // e E ě Ě
    key <AD04>  { [           r,          R,       rcaron,           Rcaron ] }; // r R ř Ř
    key <AD05>  { [           t,          T,       tcaron,           Tcaron ] }; // t T ť Ť
    key <AD06>  { [           y,          Y,       yacute,           Yacute ] }; // y Y ý Ý
    key <AD07>  { [           u,          U,      uogonek,          Uogonek ] }; // u U ų Ų
    key <AD08>  { [           i,          I,      iogonek,          Iogonek ] }; // i I į Į
    key <AD09>  { [           o,          O,        U01EB,            U01EA ] }; // o O ǫ Ǫ
    key <AD10>  { [           p,          P,       igrave,           Igrave ] }; // p P ì Ì
    key <AD11>	{ [ bracketleft,  braceleft,       eacute,           Eacute ] }; // [ { é É
    key <AD12>	{ [bracketright, braceright,       uacute,           Uacute ] }; // ] } ú Ú
    key <BKSL>	{ [       slash,        bar,       ograve,           Ograve ] }; // \ | ò Ò

    key <AC01>  { [           a,          A,        aring,            Aring ] }; // a A å Å
    key <AC02>  { [           s,          S,       scaron,           Scaron ] }; // s S š Š
    key <AC03>  { [           d,          D,       dcaron,           Dcaron ] }; // d D ď Ď
    key <AC04>  { [           f,          F,      aogonek,          Aogonek ] }; // f F ą Ą
    key <AC05>  { [           g,          G,       gcaron,           Gcaron ] }; // g G ǧ Ǧ
    key <AC06>  { [           h,          H,        uring,            Uring ] }; // h H ů Ů
    key <AC07>  { [           j,          J,       lacute,           Lacute ] }; // j J ĺ Ĺ
    key <AC08>  { [           k,          K,      lstroke,          Lstroke ] }; // k K ł Ł
    key <AC09>  { [           l,          L,       lcaron,           Lcaron ] }; // l L ľ Ľ
    key <AC10>	{ [   semicolon,      colon,        U01EB,            U01EA ] }; // ; : ó Ó
    key <AC11>	{ [  apostrophe,   quotedbl,       egrave,           Egrave ] }; // ' " á Á

    key <AB01>  { [           z,          Z,       zcaron,           Zcaron ] }; // z Z ž Ž
    key <AB02>  { [           x,          X,    zabovedot,        Zabovedot ] }; // x X ż Ż
    key <AB03>  { [           c,          C,       ccaron,           Ccaron ] }; // c C č Č
    key <AB04>  { [           v,          V,       racute,           Racute ] }; // v V ŕ Ŕ
    key <AB05>  { [           b,          B,       iacute,           Iacute ] }; // b B í Í
    key <AB06>  { [           n,          N,       nacute,           Nacute ] }; // n N ń Ń
    key <AB07>  { [           m,          M,       ncaron,           Ncaron ] }; // m M ň Ň
    key <AB08>  { [       comma,       less,       agrave,           Agrave ] }; // , < à À
    key <AB09>  { [      period,    greater,       ugrave,           Ugrave ] }; // . > ù Ù
    key <AB10>  { [   backslash, periodcentered,   egrave,           Egrave ] }; // / · è È

    key <SPCE>	{ [       space,      space, nobreakspace,     nobreakspace ] };

    include "level3(ralt_switch)"
};

