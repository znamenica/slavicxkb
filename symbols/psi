xkb_symbols "as_cyr_std" {
    name[Group1]= "AllSlavic Cyr 3-Layer";

    // TODO replace ligatures with with MULTIKEY SEQ if any
    // TODO add "<>" for slipping notation
    // TODO add "*" for markdown/ or may be develop own fotmat for cyrillic
    key <LWIN> { [       Cyrillic_io,       Cyrillic_IO, UA64F, UA64E         ] }; // ёЁꙏꙎ
    //key <TLDE> { [       Cyrillic_io,       Cyrillic_IO, UA64F, UA64E         ] }; // ёЁꙏꙎ
    key <AE01> { [            exclam,                 1, U0308,               UA67D ] }; // !(udivna) 1  ̈(dvotočje/trema) (pajerok)
    key <AE02> { [          quotedbl,                 2, U030F, U030B         ] }; // " 2  ̏(dvočašiś)   ̋ (dvostreco)
    key <AE03> { [             U0301,                 3, U0300, U2116                          ] }; // ́ (ostreco) 3  ̀(čažiś) №
    key <AE04> { [         semicolon,                 4, U0484,               None ] }; // ; 4  ҄(oblačko)
    key <AE05> { [             percent,                 5, U0483,             UA67C ] }; // % 5  ҃(titlo) (kavyka)
    key <AE06> { [          question,                 6, U0306, Greek_alpha ] }; // ? 6  ̆(kratka) α
    key <AE07> { [         parenleft,                 7, U0485,               U04A9  ] }; // ((leva otložna) 7  ҅(gusteco) ҩ
    key <AE08> { [        parenright,                 8, U0486,               U04BD  ] }; // )(prava otložna) 8  ҆(zvateľco) ҽ
    key <AE09> { [             colon,                 9, U0487, guillemotleft        ] }; // : 9  ҇(pokryťe) «
    key <AE10> { [            period,                 0, UA66F, guillemotright       ] }; // . 0  ꙯(vzmet)
    key <AE11> { [             minus,        underscore, bracketleft,         braceleft            ] }; // - _ [(leva vměstna) {
    key <AE12> { [             equal,              plus, bracketright,        braceright           ] }; // = + ](prava vměstna) }

    key <AD01> { [             UA647,             UA646, Cyrillic_je,         Cyrillic_JE          ] }; // ꙇꙆјЈ
    key <AD02> { [      Cyrillic_tse,      Cyrillic_TSE, Macedonia_dse,       Macedonia_DSE        ] }; // цЦѕЅ
    key <AD03> { [        Cyrillic_u,        Cyrillic_U, UA64B, UA64A  ] }; // уУꙋꙊ
    key <AD04> { [       Cyrillic_ka,       Cyrillic_KA, U046F, U046E        ] }; // кКѯѮ
    key <AD05> { [       Cyrillic_ie,       Cyrillic_IE, Ukrainian_ie, Ukrainian_IE ] }; // еЕєЄ
    key <AD06> { [       Cyrillic_en,       Cyrillic_EN, Cyrillic_nje,        Cyrillic_NJE         ] }; // нНњЊ
    key <AD07> { [      Cyrillic_ghe,      Cyrillic_GHE, Ukrainian_ghe_with_upturn, Ukrainian_GHE_WITH_UPTURN ] }; // гГґҐ
    key <AD08> { [      Cyrillic_sha,      Cyrillic_SHA, Cyrillic_shcha,      Cyrillic_SHCHA       ] }; // шШщЩ
    key <AD09> { [       Ukrainian_yi,        Ukrainian_YI, Ukrainian_i,       Ukrainian_I         ] }; // їЇіІ
    key <AD10> { [       Cyrillic_ze,       Cyrillic_ZE, UA641, UA640   ] }; // зЗꙁꙀ
    key <AD11> { [       Cyrillic_ha,       Cyrillic_HA, U0479,               U0478                ] }; // хХѹѸ
    key <AD12> { [ Cyrillic_hardsign, Cyrillic_HARDSIGN, U046B,               U046A   ] }; // ꙏꙎѫѪ
    key <BKSL> { [             slash,         backslash, bar,                 U0482                ] }; // / \ | ҂(tyseča)

    key <AC01> { [       Cyrillic_ef,       Cyrillic_EF, U0473,               U0472                ] }; // фФѳѲ
    key <AC02> { [     Cyrillic_yeru,     Cyrillic_YERU, Cyrillic_e,          Cyrillic_E ] }; // ыЫэЭ
    key <AC03> { [       Cyrillic_ve,       Cyrillic_VE, U0475,               U0474 ] }; // вВѵѴ
    key <AC04> { [        Cyrillic_a,        Cyrillic_A, U047B,               U047A                ] }; // аАѻѺ
    key <AC05> { [       Cyrillic_pe,       Cyrillic_PE, U0471,               U0470                ] }; // пПѱѰ
    key <AC06> { [       Cyrillic_er,       Cyrillic_ER, U047D,               U047C                ] }; // рРѽѼ
    key <AC07> { [        Cyrillic_o,        Cyrillic_O, U0461,               U0460 ] }; // оОѡѠ
    key <AC08> { [       Cyrillic_el,       Cyrillic_EL, Cyrillic_lje,        Cyrillic_LJE ] }; // лЛљЉ
    key <AC09> { [       Cyrillic_de,       Cyrillic_DE, UA649,      UA648    ] }; // дДꙉꙈ
    key <AC10> { [      Cyrillic_zhe,      Cyrillic_ZHE, Serbian_tshe,        Serbian_TSHE         ] }; // жЖҗҖ
    key <AC11> { [             U0463,             U0462, U0511, U0510 ] }; // ѣѢԑԐ

    key <AB01> { [       Cyrillic_ya,       Cyrillic_YA, UA657,               UA656                ] }; // яЯꙗꙖ
    key <AB02> { [      Cyrillic_che,      Cyrillic_CHE, Serbian_tshe,        Serbian_TSHE         ] }; // чЧћЋ
    key <AB03> { [       Cyrillic_es,       Cyrillic_ES, Cyrillic_dzhe,       Cyrillic_DZHE        ] }; // сСџЏ
    key <AB04> { [       Cyrillic_em,       Cyrillic_EM, UA659,               UA658 ] }; // мМꙙꙘ
    key <AB05> { [        Cyrillic_i,        Cyrillic_I, Cyrillic_shorti,     Cyrillic_SHORTI ] }; // иИйЙ
    key <AB06> { [       Cyrillic_te,       Cyrillic_TE, U047F,               U047E                ] }; // тТѿѾ
    key <AB07> { [ Cyrillic_softsign, Cyrillic_SOFTSIGN, U0467,               U0466 ] }; // ьЬѧѦ
    key <AB08> { [       Cyrillic_be,       Cyrillic_BE, Serbian_dje,               Serbian_DJE                ] }; // бБђЂ
    key <AB09> { [       Cyrillic_yu,       Cyrillic_YU, UA65B,               UA65A ] }; // юЮꙛꙚ
    key <AB10> { [             comma,             U00B7, U10FB,               U2056                ] }; // , · ჻ ⁖

    key <SPCE> { [       space,      space, nobreakspace,     nobreakspace ] };

    include "level3(ralt_switch)"
};
