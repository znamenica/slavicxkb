    key <AD07>  { [           u,          U,      uogonek,          Uogonek ] };
    key <AD08>  { [           i,          I,       iacute,           Iacute ] };
    key <AD09>  { [           o,          O,       ograve,           Ograve ] };
    key <AD10>  { [           p,          P,       oacute,           Oacute ] };
    key <AD11>	{ [ bracketleft,  braceleft,       uacute,           Uacute ] };
    key <AD12>	{ [bracketright, braceright,       eacute,           Eacute ] };
    key <BKSL>	{ [       slash,  backslash,          bar,          section ] };

    key <AC01>  { [           a,          A,        aring,            Aring ] };
