    //        onehalf,   logicaland ]	};
    //         threequarters ] };
    //         plusminus ]	};
    //      ellipsis,            U0335 ] }; // … (ellipsis), isčerk(stroke)
    key <LWIN>	{ [  asciitilde,      grave,        acute,       apostrophe ] }; 
    key <AE01>  { [      exclam,          1,     EuroSign,           degree ] }; //
    key <AE02>  { [    quotedbl,          2,           at,            U030C ] }; // haček (caron)
    key <AE03>  { [       U0301,          3,   numbersign,            U0303 ] }; // acute (ostreco), circumflex (^)
    key <AE04>  { [   semicolon,          4,       dollar,            U0306 ] }; // breve (kratco)
    key <AE05>  { [     percent,          5,        U0303,            U030A ] }; // tilde (titlo), above ring (nadkolco)
    key <AE06>  { [    question,          6,  asciicircum,            U0328 ] }; // ogonek
    key <AE07>  { [   parenleft,          7,    ampersand,            U0300 ] }; // grave(čažiś)
    key <AE08>  { [  parenright,          8,     asterisk,            U0307 ] }; // abovedot
    key <AE09>  { [       colon,          9,  bracketleft,    guillemotleft ] };
    key <AE10>  { [      period,          0, bracketright,   guillemotright ] };
    key <AE11>	{ [       minus, underscore,        U030B,            U0304 ] }; // dvostreco, macron (nadčerk)
    key <AE12>	{ [       equal,       plus,        U0308,            U0327 ] }; // dvokrapka (diaeresis), cedilla

    key <AD01>  { [           q,          Q,      eogonek,          Eogonek ] };
